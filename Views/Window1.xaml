<Window x:Class="Zoo.Views.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodels="clr-namespace:Zoo.View_Models"
        xmlns:local="clr-namespace:Zoo.Views"
        mc:Ignorable="d"
        Title="Window1" Height="600" Width="900">
    <Window.DataContext>
        <viewmodels:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <ContentControl Content="{Binding SelectedViewModel}"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,173,0,0" VerticalAlignment="Top" Height="78" Width="168" Command="{Binding UpdateViewCommand}" CommandParameter="Login"/>

    </Grid>
</Window>
